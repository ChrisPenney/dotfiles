[user]
	name = Ansel Santosa
	email = ansel@anselsantosa.com
[color]
	ui = auto
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%C(yellow)%h %C(blue)%ad %C(red)%d %C(reset)%s%d %C(green)[%an]\" --decorate --graph --date=short
  type = cat-file -t
  dump = cat-file -p
